{
  "expo": {
    "name": "AstroRoshni",
    "slug": "astroroshni-mobile",
    "version": "1.1.6",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f97316"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
