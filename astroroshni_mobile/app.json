      "deploymentTarget": "15.1",
      "newArchEnabled": false,
      "bitcode": false,
    "infoPlist": {
      "ITSAppUsesNonExemptEncryption": false,
      "NSAppTransportSecurity": {
        "NSAllowsArbitraryLoads": true
      },
      "NSMicrophoneUsageDescription": "This app does not use the microphone.",
      "NSSpeechRecognitionUsageDescription": "This app uses speech recognition for voice commands.",
      "UILaunchScreen": {}
    }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#f97316"
      },
      "package": "com.astroroshni.mobile",
      "versionCode": 79,
      "newArchEnabled": false,
      "googleServicesFile": "./google-services.json",
      "permissions": ["com.android.vending.BILLING"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8e33070b-ac6c-42e0-a089-bd830019bb1a"
      }
    },
    "runtimeVersion": "1.1.6",
    "updates": {
      "url": "https://u.expo.dev/8e33070b-ac6c-42e0-a089-bd830019bb1a"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/android-notification-icon.png",
          "color": "#f97316"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ]
  }
}