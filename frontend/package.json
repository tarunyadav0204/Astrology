{
  "name": "astrology-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@react-pdf/renderer": "^4.3.1",
    "axios": "^1.6.0",
    "date-fns": "^2.30.0",
    "gtag": "^1.0.1",
    "jspdf": "^2.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.4",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.9.4",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.1.0"
  },
  "scripts": {
    "start": "PORT=3001 react-scripts start",
    "build": "react-scripts build && react-snap",
    "build:dev": "NODE_ENV=development react-scripts build",
    "build:prod": "NODE_ENV=production react-scripts build && react-snap"
  },
  "reactSnap": {
    "include": [
      "/",
      "/marriage-analysis",
      "/career-guidance",
      "/health-analysis",
      "/wealth-analysis",
      "/panchang",
      "/muhurat-finder",
      "/nakshatras",
      "/blog"
    ],
    "crawl": true,
    "waitFor": 5000,
    "puppeteerArgs": ["--no-sandbox", "--disable-setuid-sandbox"],
    "minifyHtml": {
      "collapseWhitespace": false,
      "removeComments": false
    },
    "skipThirdPartyRequests": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "proxy": "http://localhost:8001",
  "devDependencies": {
    "react-snap": "^1.23.0"
  }
}
